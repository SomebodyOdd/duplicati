<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyOriginatorKeyFile>Duplicati.snk</AssemblyOriginatorKeyFile>
    <FileUpgradeFlags />
    <UpgradeBackupLocation />
    <TargetFramework>net471</TargetFramework>
    <UseMSBuildEngine>false</UseMSBuildEngine>
    <AssemblyTitle>Duplicati.Library.Interfaces</AssemblyTitle>
    <Company>Duplicati Team</Company>
    <Product>Duplicati.Library.Interfaces</Product>
    <Description>The Duplicati library with all publicly exposed interfaces</Description>
    <Copyright>LGPL, Copyright © Duplicati Team 2015</Copyright>
    <AssemblyVersion>2.0.0.7</AssemblyVersion>
    <FileVersion>2.0.0.7</FileVersion>
    <OutputPath>bin\$(Configuration)\</OutputPath>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugType>full</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>pdbonly</DebugType>
  </PropertyGroup>
  <ItemGroup>
    <None Include="Duplicati.snk" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Localization\Duplicati.Library.Localization.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="IBackendGUI.cs" />
    <Compile Remove="ICompressionGUI.cs" />
    <Compile Remove="IEncryptionGUI.cs" />
    <Compile Remove="IGUIControl.cs" />
    <Compile Remove="IGUIMiniControl.cs" />
    <Compile Remove="ISettingsControl.cs" />
  </ItemGroup>
</Project>