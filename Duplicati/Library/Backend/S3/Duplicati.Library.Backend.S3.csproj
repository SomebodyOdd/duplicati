<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <RootNamespace>Duplicati.Library.Backend</RootNamespace>
    <AssemblyOriginatorKeyFile>Duplicati.snk</AssemblyOriginatorKeyFile>
    <FileUpgradeFlags />
    <TargetFramework>net471</TargetFramework>
    <UpgradeBackupLocation />
    <UseMSBuildEngine>false</UseMSBuildEngine>
    <AssemblyTitle>S3</AssemblyTitle>
    <Company>Duplicati Team</Company>
    <Product>Duplicati.Backend.S3</Product>
    <Description>An S3-compatible backend for Duplicati</Description>
    <Copyright>LGPL, Copyright © Duplicati Team 2015</Copyright>
    <AssemblyVersion>2.0.0.7</AssemblyVersion>
    <FileVersion>2.0.0.7</FileVersion>
    <OutputPath>bin\$(Configuration)\</OutputPath>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugType>full</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>pdbonly</DebugType>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="AWSSDK.Core" Version="3.3.103.37" />
    <PackageReference Include="AWSSDK.IdentityManagement" Version="3.3.103.26" />
    <PackageReference Include="AWSSDK.S3" Version="3.3.104.25" />
    <PackageReference Include="Minio" Version="3.1.7" />
    <PackageReference Include="RestSharp" Version="106.3.1" />
    <PackageReference Include="System.Reactive" Version="4.0.0" />
    <PackageReference Include="System.Reactive.Linq" Version="4.0.0" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Web" />
    <Reference Include="System.Windows" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="WindowsBase" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\Utility\Duplicati.Library.Utility.csproj" />
    <ProjectReference Include="..\..\Interface\Duplicati.Library.Interface.csproj" />
    <ProjectReference Include="..\..\Localization\Duplicati.Library.Localization.csproj" />
    <ProjectReference Include="..\..\Logging\Duplicati.Library.Logging.csproj" />
    <ProjectReference Include="..\..\Common\Duplicati.Library.Common.csproj" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Duplicati.snk" />
  </ItemGroup>
</Project>