<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyOriginatorKeyFile>Duplicati.snk</AssemblyOriginatorKeyFile>
    <FileUpgradeFlags />
    <TargetFramework>net471</TargetFramework>
    <UpgradeBackupLocation />
    <UseMSBuildEngine>false</UseMSBuildEngine>
    <ProductVersion>8.0.30703</ProductVersion>
    <AssemblyTitle>Duplicati.Core</AssemblyTitle>
    <Company>Duplicati Team</Company>
    <Product>Duplicati.Core</Product>
    <Description>A collection of utility functions used extensively in Duplicati</Description>
    <Copyright>LGPL, Copyright © Duplicati Team 2015</Copyright>
    <AssemblyVersion>2.0.0.7</AssemblyVersion>
    <FileVersion>2.0.0.7</FileVersion>
    <OutputPath>bin\$(Configuration)\</OutputPath>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugType>full</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>pdbonly</DebugType>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="FasterHashing" Version="1.3.0" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System.Management" />
    <Reference Include="System.Windows.Forms" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Duplicati.snk" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Common\Duplicati.Library.Common.csproj" />
    <ProjectReference Include="..\Localization\Duplicati.Library.Localization.csproj" />
    <ProjectReference Include="..\Interface\Duplicati.Library.Interface.csproj" />
  </ItemGroup>
</Project>