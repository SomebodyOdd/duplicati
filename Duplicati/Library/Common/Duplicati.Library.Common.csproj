<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <RootNamespace>Duplicati.Library.IO</RootNamespace>
    <AssemblyName>Duplicati.Library.IO</AssemblyName>
    <AssemblyOriginatorKeyFile>Duplicati.snk</AssemblyOriginatorKeyFile>
    <FileUpgradeFlags />
